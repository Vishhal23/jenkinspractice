    agent any

    stages {
        stage('Build') {
            steps {
                echo 'This is an Build Job'
            }
        }
     stage('Deploy') {
            steps {
                echo 'This is an Deploy Job'
            }
        }

        stage('Test') {
            steps {
                echo 'This is an Test Job'
            }
        }

 stage('Release') {
            steps {
                echo 'This is an Release Job'
            }
        }
}
}
